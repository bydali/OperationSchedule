		</ScrollPane>
		<ScrollPane fx:id="timeSp" hbarPolicy="NEVER" prefHeight="40.0" prefWidth="200.0" vbarPolicy="NEVER" GridPane.columnIndex="1">
			<content>
				<AnchorPane fx:id="timeAp" prefHeight="40.0" />
			</content>
		</ScrollPane>
		<ScrollPane fx:id="sp" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="16.0" prefWidth="1093.0" vbarPolicy="NEVER" GridPane.columnIndex="1" GridPane.rowIndex="2">
			<content>
				<AnchorPane fx:id="ap" prefHeight="21.0" />
			</content>
		</ScrollPane>
	</children>
	<BorderPane.margin>
		<Insets />
	</BorderPane.margin>
